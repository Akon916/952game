#person_center_pst{
    width:250px;
    height: 0px;
    overflow: hidden;
    position: absolute;
    left: -0px;
    top: 47px;
    z-index: 10;
    background: #FFFFFF;
    border-radius: 5px;
    -webkit-box-shadow: 0 4px 8px 0 rgba(223, 223, 223, .50);
            box-shadow: 0 4px 8px 0 rgba(223, 223, 223, .50);
    text-align: center;
}

.h_login_en .header_link>a{
    display: block;
    width: 64px;
    height: 64px;
    margin-left: 105px;
}
.h_login_en .header_link>a>img{
    display: block;
    width: 32px;
    height: 32px;
    position: absolute;
    top:7px;
    left: 105px;
    z-index: 100;
    border-radius: 50%;
    overflow: hidden;
    -webkit-transition: all .3s;
    transition: all .3s;
}
.person_show{
    transform:scale(2);
    -webkit-transform:scale(2);
    -moz-transform:scale(2);
    -o-transform:scale(2);
    transform-origin: top center;
    -webkit-transform-origin: top center;
    -moz-transform-origin: top center;
    -o-transform-origin: top center;
}
#person_center_pst>div{
    width: 100%;
    height: 50px;
}
#person_name{
	margin-top 15px
    font-size: 15px;
    font-weight: bold;
}
